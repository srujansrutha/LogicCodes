   "source": [
    "def factorial(n):\n",
    "    fact=1\n",
    "    if n==1 or n==0:\n",
    "        return 1\n",
    "    else:\n",
    "        for i in range(1,n+1):\n",
    "            fact *= i\n",
    "        return fact\n",
    "n=int(input(\"enter a number : \"))\n",
    "a=factorial(n)\n",
    "print(a)"
   ]
